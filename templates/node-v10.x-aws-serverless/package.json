    "test:once": "NODE_ENV=test yarn sls:dotenv && jest",
    "test:coverage": "NODE_ENV=test yarn sls:dotenv && jest --coverage",
    "test:coverage:serie": "NODE_ENV=test yarn sls:dotenv && jest --coverage --runInBand"
